<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PenScan.Views.ProjectPhases.PreEngagementPage">
    <ContentPage.Content>
        <StackLayout Padding="30">
            <Label Text="Contact Id:"/>
            <Entry Text="{Binding ContractId}" IsReadOnly="False"/>

            <Label Text="Akkoord met contract?"/>
            <CheckBox x:Name="ContractSignedCheckBox"/>

            <Button x:Name="ContractConfirmButton" Text="Confirm" Command="{Binding ConfirmCommand}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>